<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.ListView?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.mermil.erp.controllers.stock.UploadStockController"
            prefHeight="400.0" prefWidth="600.0" minWidth="600" maxWidth="600" minHeight="400" maxHeight="400">
    
    <children>
        <Button fx:id="uploadButton" layoutX="250.0" layoutY="350.0" text="Upload File" onAction="#handleUploadButtonClick"/>
        <Label fx:id="statusLabel" layoutX="250.0" layoutY="370.0" />
        <ListView fx:id="productListView" layoutX="10.0" layoutY="10.0" prefHeight="320.0" prefWidth="580.0"/>
    </children>

    
</AnchorPane>